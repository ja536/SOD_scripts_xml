<!-- J. Aragon 2017180. For requesting broadband channels from IRIS DMC for use in Splitlab for Matlab.
to initiate download in working direcory use ~> sod -f SOD_Splitlab.xml
Select file format yyyy.DDD.HH.mm.SAC.e in splitlab-->
<?xml version="1.0" encoding="UTF-8"?>
<sod>
 <eventArm>
    <fdsnEvent>
        <originTimeRange>
            <startTime>
                <year>2016</year>
                <month>06</month>
                <day>1</day>
            </startTime>
            <endTime>
                <year>2016</year>
                <month>12</month>
                <day>30</day>
            </endTime>
        </originTimeRange>
        <magnitudeRange>
            <min>5.7</min>
        </magnitudeRange>
    </fdsnEvent>
<printlineEventProcess/>
</eventArm>
<networkArm>
    <fdsnStation/>
    <networkCode>TA</networkCode>
    <stationOR>
		<stationCode>N49A</stationCode>
		<stationCode>051A</stationCode>
		<stationCode>052A</stationCode>
		<stationCode>P53A</stationCode>
		<stationCode>Q55A</stationCode>
    </stationOR>
    <channelCode>BH*</channelCode>
<printlineChannelProcess/>
</networkArm>
<waveformArm>
    <distanceRange>
        <unit>DEGREE</unit>
        <min>90</min>
        <max>130</max>
    </distanceRange>
    <originOffsetRequest>
        <beginOffset>
        <unit>SECOND</unit>
        <value>0</value>
        </beginOffset>
        <endOffset>
        <unit>SECOND</unit>
        <value>2400</value>
        </endOffset>
    </originOffsetRequest>
    <sacWriter>
        <workingDir>seismograms</workingDir> <location>${station.code}/${event.getTime("yyyy.DDD.HH.mm.ss")}.${station.code}.SAC.${channel.code}</location>
        <phaseTimeHeader>
            <model>prem</model>
            <phaseName>ttp</phaseName>
            <tHeader>a</tHeader>
        </phaseTimeHeader>
    </sacWriter>
<printlineSeismogramProcess/>
</waveformArm>
</sod>
